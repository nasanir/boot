package pers.nasanir.blog.common.reflect;

import java.lang.reflect.InvocationTargetException;
import java.lang.reflect.Method;

public class Invoke {
	private Method method;
	
	
	
	public void invoke(Object obj,Object[] args) throws IllegalAccessException, IllegalArgumentException, InvocationTargetException{
		this.method.invoke(obj, args);
	}
}
